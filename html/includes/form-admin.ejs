<h1>ADMIN CONTROL</h1>
<ul id="admin-wrapper">
  <li id="command-list-item">
    <label for="commandType">Command</label>
    <select name="commandType" id="commandType">
      <option value="startScrape" id="startScrape" selected>Start Scrape</option>
      <option value="stopScrape" id="stopScrape">Stop Scrape</option>
      <option value="scrapeStatus" id="scrapeStatus">Get Scrape Status</option>
    </select>
  </li>
  <li id="itemType-list-item">
    <label for="itemType">Item Type</label>
    <select name="itemType" id="itemType">
      <option value="everythingType" id="everythingType" selected>Everything</option>
      <option value="articlesType" id="articlesType">Just Articles</option>
      <option value="picsType" id="picsType">Just Pic Sets</option>
      <option value="vidsType" id="vidsType">Just Vids</option>
    </select>
  </li>

  <li id="urlInput-list-item" class="hidden">
    <label for="urlInput">URL</label>
    <input type="text" name="urlInput" id="urlInput" />
  </li>
  <li id="howMany-list-item">
    <label for="howMany">How Many?</label>
    <input type="text" name="howMany" id="howMany" placeholder="[Defaults to 1 (most recent)]" />
  </li>
  <li id="newData-list-item">
    <label for="pullNewData">Re-Scrape?</label>
    <select name="pullNewData" id="pullNewData">
      <!-- <option value="" disabled selected>[Defaults to No, will take few min if yes]</option> -->
      <option value="noNewData" id="noNewData" selected>No</option>
      <option value="yesNewData" id="yesNewData">Yes [will take a few min]</option>
    </select>
  </li>
</ul>
<button id="admin-submit-button">Submit</button>
